{% for flashesKey, flashesValue in app.flashes %}
    {% for message in flashesValue %}
        <div class="alert alert-{{ flashesKey }} alert-dismissible fade show">
            {{ message|trans|raw }}
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    {% endfor %}
{% endfor %}
